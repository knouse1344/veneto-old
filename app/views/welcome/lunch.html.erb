	<div class="bord_top"> </div>

	<div class="bord_middle">
		<div class="title"> 
			<h1> Menu </h1>
		</div>

		<div id="navi">
			 <a class="selected" href="/welcome/lunch"> Lunch </a>
			 <a style="margin-left: 275px;" href="/welcome/dinner"> Dinner </a>
			 <a style="margin-left: 275px;" href="/welcome/drinks"> Drinks </a>
			 <div class="menu_garnish one"> </div>
			 <div class="menu_garnish two"> </div>
